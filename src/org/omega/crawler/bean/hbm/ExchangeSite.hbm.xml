<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.omega.crawler.bean.ExchangeSiteBean" table="exchange_site" dynamic-insert="true" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <property name="name" type="string">
            <column name="name" />
        </property>
        <property name="websiteUrl" type="string">
            <column name="website_url" />
        </property>
        <property name="coinStatsUrl" type="string">
            <column name="coin_stats_url" />
        </property>
        
        <property name="fee" type="java.lang.Double">
            <column name="fee" />
        </property>
        
        
    </class>
</hibernate-mapping>
