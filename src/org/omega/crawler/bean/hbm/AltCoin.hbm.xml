<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.omega.crawler.bean.AltCoinBean" table="alt_coin" dynamic-insert="true" dynamic-update="true">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="topicid" type="java.lang.Integer">
            <column name="topic_id" />
        </property>
        <property name="author" type="string">
            <column name="author" length="60" />
        </property>
        <property name="title" type="string">
            <column name="title" length="255" />
        </property>
        <property name="link" type="string">
            <column name="link" length="180" />
        </property>
        <property name="publishDate" type="date">
            <column name="publish_date" length="0"/>
        </property>
        <property name="replies" type="java.lang.Integer">
            <column name="replies" />
        </property>
        <property name="views" type="java.lang.Integer">
            <column name="views" />
        </property>
        <property name="publishContent" type="text">
            <column name="publish_content" length="16777216" />
        </property>
		<property name="isParsed" type="java.lang.Boolean">
            <column name="is_parsed" />
        </property>
        
        <property name="algo" type="string">
            <column name="algo" length="45" />
        </property>
        <property name="countDown" type="date">
            <column name="count_down" length="0"/>
        </property>
         <property name="name" type="string">
            <column name="name" length="100" />
        </property>
         <property name="abbrName" type="string">
            <column name="abbr_name" length="20" />
        </property>
        <property name="interestLevel" type="java.lang.Integer">
            <column name="interest_level" />
        </property>
        
        <property name="proof" type="string">
            <column name="proof" length="45" />
        </property>
        <property name="cpuMinable" type="java.lang.Boolean">
            <column name="cpu_minable" />
        </property>
        <property name="gpuMinable" type="java.lang.Boolean">
            <column name="gpu_minable" />
        </property>
        <property name="asicMinable" type="java.lang.Boolean">
            <column name="asic_minable" />
        </property>
        
    </class>
</hibernate-mapping>
